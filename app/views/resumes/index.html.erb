<h1>Listing resumes</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Summary</th>
    <th>Phone number</th>
    <th>Email</th>
    <th>Date of birth</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @resumes.each do |resume| %>
  <tr>
    <td><%= resume.name %></td>
    <td><%= resume.summary %></td>
    <td><%= resume.phone_number %></td>
    <td><%= resume.email_id %></td>
    <td><%= resume.date_of_birth %></td>
    <td><%= link_to 'Show', resume %></td>
    <td><%= link_to 'Edit', edit_resume_path(resume) %></td>
    <td><%= link_to 'Destroy', resume, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Resume', new_resume_path %>
